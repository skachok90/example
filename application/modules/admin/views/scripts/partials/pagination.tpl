<?php $pagePar = $this->page ? $this->page : 'page'; ?><?php if ($this->pageCount > 1) { ?>	<nav>		<ul class="pagination"> 			<?php if ($this->firstPageInRange > 1)	{ ?>				<li><a href="<?php echo $this->customUrl('', array($pagePar => 1)); ?>" class="button gray">&laquo; First</a></li>			<?php } ?>						<?php if (isset($this->previous))	{ ?>				<li><a href="<?php echo $this->customUrl('', array($pagePar => $this->previous)); ?>" class="button gray">&lt; Prev</a></li>			<?php } ?>						<?php foreach ($this->pagesInRange as $page) { ?>				<li>					<a href="<?php echo $this->customUrl('', array($pagePar => $page)); ?>" class="button <?php echo $page != $this->current ? 'gray' : '' ?>">						<?php echo $page; ?>					</a>				</li>			<?php } ?>						<?php if (isset($this->next))	{ ?>				<li><a href="<?php echo $this->customUrl('', array($pagePar => $this->next)); ?>" class="button gray">Next &gt;</a></li>			<?php } ?>						<?php if ($this->lastPageInRange < $this->pageCount)	{ ?>				<li><a href="<?php echo $this->customUrl('', array($pagePar => $this->last)); ?>" class="button gray">Last &raquo;</a></li>			<?php } ?>		</ul>	</nav><?php } ?>